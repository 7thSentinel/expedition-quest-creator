{
  "name": "expedition-quest-creator",
  "version": "1.1.0",
  "description": "IDE for Expedition Quest Creation",
  "repository": "https://github.com/Fabricate-IO/expedition-quest-creator",
  "main": "react.js",
  "scripts": {
    "start": "node ${SCRIPT:-app.js}",
    "dev-dash": "NODE_ENV=dev webpack-dashboard -- node ${SCRIPT:-app.js}",
    "dev": "NODE_ENV=dev node ${SCRIPT:-app.js}",
    "monitor": "nodemon ${SCRIPT:-app.js}",
    "deploy": "gcloud app deploy app.yaml",
    "test": "node node_modules/.bin/mocha --colors --compilers ts:ts-node/register --compilers tsx:ts-node/register --recursive ./app/**/**/**/*.test.tsx ./app/**/**/*.test.tsx ./app/**/*.test.tsx ./app/*.test.tsx ./errors/*.test.tsx ./models/*.test.js ./errors/generateDocs.tsx",
    "watch:test": "watch-run -i -p \"app/**\" npm run test",
    "init-cloudsql": "node books/model-cloudsql.js"
  },
  "author": "Fabricate.IO",
  "license": "CC-BY-NC-SA-4.0",
  "semistandard": {
    "globals": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "it"
    ]
  },
